{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: main.py with serial port",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/main.py",
            "console": "integratedTerminal",
            "args": [
                "--port",
                "/dev/ttyACM0"
            ]
        }
    ]
}