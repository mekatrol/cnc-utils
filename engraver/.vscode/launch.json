{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: engraver.py",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/src/ide.py",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "env": {
        "PYTHONPATH": "${workspaceFolder}/src"
      },
      "args": [
        "--input",
        "samples/six.svg",
        "--scale",
        "10000",
        "--tol",
        "0.1",
        "--export-json",
        "output/six-toolpaths.json"
      ],
      "justMyCode": true
    },
    {
      "name": "Python: pytest",
      "type": "debugpy",
      "request": "launch",
      "module": "pytest",
      "cwd": "${workspaceFolder}",
      "env": {
        "PYTHONPATH": "${workspaceFolder}/src"
      },
      "args": ["-q"]
    }
  ]
}
